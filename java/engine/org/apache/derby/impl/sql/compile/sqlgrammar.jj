/*
 * <A NAME="autoIncrementBeginEnd">autoIncrementBeginEnd</A>
 */
void 
autoIncrementBeginEnd(long[] autoIncrementInfo) throws StandardException :
{
	 Object[] optionalClauses = new Object[ 3 ];
}
{
	( LOOKAHEAD({getToken(1).kind == COMMA ||
	                   getToken(1).kind == START || getToken(1).kind == INCREMENT
                                            || getToken(1).kind == CYCLE })
	
    		( identityColumnOption( autoIncrementInfo , optionalClauses) ) *
    	)
      
}

/*
 * <A NAME="identityColumnOption">identityColumnOption</A>
 */
void 
identityColumnOption(long[] autoIncrementInfo,Object[] optionalClauses) throws StandardException :
{
	int  optionIndex = -1;
	long		autoIncrementInitial = 1;
	long		autoIncrementIncrement = 1;
	long            autoIncrementCycle = 0;
	Token optionToken = null;
      
}
{
    (
        optionToken = <START> <WITH> autoIncrementInitial = exactNumber()
	{autoIncrementInfo[QueryTreeNode.AUTOINCREMENT_START_INDEX] = autoIncrementInitial;
         autoIncrementInfo[QueryTreeNode.AUTOINCREMENT_CREATE_MODIFY] = ColumnDefinitionNode.CREATE_AUTOINCREMENT;	 
	optionIndex = 0;}
        |
        optionToken = <INCREMENT> <BY> autoIncrementIncrement = exactNumber()
	{autoIncrementInfo[QueryTreeNode.AUTOINCREMENT_INC_INDEX] = autoIncrementIncrement;
         autoIncrementInfo[QueryTreeNode.AUTOINCREMENT_CREATE_MODIFY] = ColumnDefinitionNode.CREATE_AUTOINCREMENT;
        optionIndex = 1;} 
	|
	optionToken = <CYCLE> 
	{autoIncrementCycle = 1;
	autoIncrementInfo[QueryTreeNode.AUTOINCREMENT_CYCLE] = autoIncrementCycle;
	optionIndex = 2;}
	|
	optionToken = <NO> <CYCLE> 
	{autoIncrementCycle = 0;
	autoIncrementInfo[QueryTreeNode.AUTOINCREMENT_CYCLE] = autoIncrementCycle;
	optionIndex = 2;}
	|
	<COMMA> ( LOOKAHEAD({getToken(1).kind == COMMA ||
	                   getToken(1).kind == RIGHT_PAREN})
	{optionIndex = -2;}
	|
	{return;}
        )
	
        
    )
   {
        if ( optionIndex != -1)
        {
	    if(optionIndex == -2)
	    {
		throw StandardException.newException(SQLState.LANG_SYNTAX_ERROR, "Encountered an \",\" ");
	    }
            if ( optionalClauses[ optionIndex ] != null )
            {
                throw StandardException.newException( SQLState.LANG_DUPLICATE_CS_CLAUSE, optionToken.image );
            }

            optionalClauses[ optionIndex ] = optionToken;
        }
    }
                     
}

