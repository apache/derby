            if ( hasJDBClimitClause ) { tokenName = "LIMIT"; }
            else if ( offsetClauses[ OFFSET_CLAUSE ] != null ) { tokenName = "OFFSET"; }
            else { tokenName = "FETCH"; }

            throw StandardException.newException( SQLState.LANG_SYNTAX_ERROR, tokenName );
