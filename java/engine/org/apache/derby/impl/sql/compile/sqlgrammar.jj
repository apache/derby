	(
                isolationLevel = isolationLevelDB2Abbrev() { return isolationLevel; }
                | ( ( <REPEATABLE> <READ> ) | <SERIALIZABLE> )
                        { return TransactionControl.SERIALIZABLE_ISOLATION_LEVEL; }
		| <CURSOR> <STABILITY>
                        { return TransactionControl.READ_COMMITTED_ISOLATION_LEVEL; }

