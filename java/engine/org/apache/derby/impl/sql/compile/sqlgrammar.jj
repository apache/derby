							targetTable,
							columnList,
							queryExpression,
							null,
							targetProperties,
							orderCols,
                            offsetClauses[ OFFSET_CLAUSE ],
                            offsetClauses[ FETCH_FIRST_CLAUSE ],
                            hasJDBClimitClause,
							getContextManager());
