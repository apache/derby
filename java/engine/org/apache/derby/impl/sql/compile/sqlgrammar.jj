									(TableElementList) nodeFactory.getNode(
												C_NodeTypes.TABLE_ELEMENT_LIST,
												getContextManager());
	Token				tok = null;
	int[]				changeType = new int[1];
	int[]				behavior = new int[1];
	boolean[]			sequential = new boolean[1];
}
{
